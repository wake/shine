{
  "name": "wake/shine",
  "description": "A starter project with Lumen core and Silex development style.",
  "keywords": ["framework", "lumen", "silex"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "laravel/lumen-framework": "5.7.*",
    "illuminate/session": "^5.7",
    "illuminate/validation": "^5.7",
    "vlucas/phpdotenv": "~2.2",
    "symfony/var-dumper": "dev-master",
    "rcrowe/twigbridge": "^0.9.6",
    "robmorgan/phinx": "~0.0",
    "j4mie/idiorm": "~1.0",
    "j4mie/paris": "~1.0",
    "wake/shadow": "dev-master",
    "wake/pager": "dev-master",
    "wake/php-array-kit": "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "resource/src/app/"
    },
    "files": [
      "resource/src/app/Helper/Function.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php -r \"file_exists('public/.htaccess.local') || rename('public/.htaccess.local', 'public/.htaccess');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
